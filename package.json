    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-helmet": "^6.1.0",
    "@types/react-pdf": "^5.0.9",
    "@types/react-slick": "^0.23.10",
    "@types/react-truncate": "^2.3.4",
    "@types/uuid": "^9.0.0",
    "@umijs/fabric": "^2.5.1",
    "@umijs/plugin-blocks": "^2.0.5",
