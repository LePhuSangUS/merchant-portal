{
  "name": "merchant-portal",
  "version": "4.5.0",
  "private": true,
  "description": "Một nền tảng - Vạn hành trình - Khai mở tiềm năng thanh toán số",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build:dev": "umi build",
    "build:uat": "umi build",
    "build:sandbox": "umi build",
    "build:preprod": "umi build",
    "build:old": "cross-env REACT_APP_ENV=uat REACT_APP_ROUTE=/portal UMI_ENV=uat umi build",
    "start": "npm run dev",
    "dev": "cross-env REACT_APP_ENV=dev REACT_APP_ROUTE=/portal UMI_ENV=dev umi dev",
    "uat": "cross-env REACT_APP_ENV=uat REACT_APP_ROUTE=/portal UMI_ENV=uat umi dev",
    "sandbox": "cross-env REACT_APP_ENV=sandbox REACT_APP_ROUTE=/portal UMI_ENV=sandbox umi dev",
    "preprod": "cross-env REACT_APP_ENV=preprod REACT_APP_ROUTE=/portal UMI_ENV=preprod umi dev",
    "test": "umi generate tmp && cross-env REACT_APP_ENV=test UMI_ENV=test umi test --silent --coverage",
    "test:local": "cross-env REACT_APP_ENV=test UMI_ENV=test umi test",
    "sonar": "node sonar-project.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.2.11",
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.2.0",
