    "mockjs": "^1.0.1-beta3",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "prop-types": "^15.5.10",
    "qrcode.react": "^1.0.1",
    "randomatic": "^3.1.1",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-excel-renderer": "^1.1.0",
    "react-fast-compare": "^3.2.0",
    "react-fittext": "^1.0.0",
    "react-gcolor-picker": "^1.2.1",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet-async": "^1.0.4",
    "react-html-parser": "^2.0.2",
    "react-loading": "^2.0.3",
